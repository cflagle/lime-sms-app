# Database
DATABASE_URL=file:./dev.db

# Lime API Credentials
LIME_USER=your-lime-email@example.com
LIME_API_ID=your-lime-api-id

# App Authentication - Primary User
APP_USERNAME=your-username
APP_PASSWORD=your-secure-password

# App Authentication - Secondary User (optional)
APP_USERNAME_2=secondary-username
APP_PASSWORD_2=secondary-secure-password

# Cron Job Secret
CRON_SECRET=your-cron-secret

# Docker/PostgreSQL (optional - for docker-compose)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-postgres-password
POSTGRES_DB=lime_sms_db
